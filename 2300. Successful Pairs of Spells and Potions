class Solution:
    def successfulPairs(self, spells: List[int], potions: List[int], success: int) -> List[int]:
        potions.sort()
        result = []
        for s in spells:
            left, right = 0, len(potions) - 1
            while left <= right:
                m = (left + right) // 2
                if s * potions[m] >= success:
                    right = m - 1
                else:
                    left = m + 1
            result.append(len(potions) - left)
        return result
